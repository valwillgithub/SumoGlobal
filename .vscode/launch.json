{
	"configurations": [
		{
			"name": "sumo global debugging",
			"type": "node",
			"request": "launch",
			"args": ["wdio.conf.js", "--spec", "${file}"],
			"cwd": "${workspaceFolder}",
			"autoAttachChildProcesses": true,
			"program": "${workspaceRoot}/node_modules/@wdio/cli/bin/wdio.js",
			"console": "integratedTerminal"
		}
	]
}
